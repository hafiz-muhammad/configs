# Tuned profiles
alias tuned-act="tuned-adm active" # Show active profile
alias tuned-rec="tuned-adm recommend" # Recommend a profile
alias tuned-ps="tuned-adm profile powersave" # Set powersave profile
alias tuned-bb="tuned-adm profile balanced-battery" # Set balanced-battery profile
alias tuned-b="tuned-adm profile balanced" # Set balanced profile
alias tuned-tp="tuned-adm profile throughput-performance" # Set throughput-performance profile
