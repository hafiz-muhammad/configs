# fwupdmgr 
alias fwgup='fwupdmgr get-updates'
alias fwup='fwupdmgr update'
alias fwdevs='sudo fwupdmgr get-devices'
alias fwhist='sudo fwupdmgr get-history'

alias bios-ver='sudo dmidecode -s bios-version'
alias bios-reldate='sudo dmidecode -s bios-release-date'
alias bios-info='sudo dmidecode -t bios'